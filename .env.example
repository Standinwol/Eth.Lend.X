# Infura Configuration
INFURA_API_KEY=your_infura_api_key_here
INFURA_SEPOLIA_API_URL=https://sepolia.infura.io/v3/your_project_id_here

# Mainnet Account Private Key
MAIN_ACCOUNT=your_private_key_here

# CoinMarketCap API for gas reporting (optional)
COINMARKETCAP=your_coinmarketcap_api_key_here

# Next.js Environment Variables for Frontend
NEXT_PUBLIC_INFURA_API_URL=https://sepolia.infura.io/v3/your_project_id_here
NEXT_PUBLIC_NETWORK_ID=11155111

# Contract Addresses (update these after deployment)
NEXT_PUBLIC_LENDING_POOL_ADDRESS=your_lending_pool_address_here
NEXT_PUBLIC_LENDING_HELPER_ADDRESS=your_lending_helper_address_here
NEXT_PUBLIC_LENDING_CONFIG_ADDRESS=your_lending_config_address_here
NEXT_PUBLIC_ADDRESS_TO_TOKEN_MAP_ADDRESS=your_address_to_token_map_address_here
NEXT_PUBLIC_DAI_TOKEN_ADDRESS=your_dai_token_address_here
NEXT_PUBLIC_USDC_TOKEN_ADDRESS=your_usdc_token_address_here
NEXT_PUBLIC_LINK_TOKEN_ADDRESS=your_link_token_address_here